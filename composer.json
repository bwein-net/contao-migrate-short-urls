{
  "name": "bwein-net/contao-migrate-short-urls",
  "description": "Migration of short URLs to URL rewrites for Contao Open Source CMS",
  "keywords": [
    "contao",
    "migrate",
    "migration",
    "fritzmg",
    "short",
    "urls",
    "terminal42",
    "url",
    "rewrite",
    "redirect"
  ],
  "type": "contao-bundle",
  "homepage": "https://github.com/bwein-net/contao-migrate-short-urls",
  "license": "MIT",
  "authors": [
    {
      "name": "bwein.net",
      "homepage": "https://bwein.net/",
      "role": "Developer"
    },
    {
      "name": "Ralf Baussenwein",
      "homepage": "https://github.com/rabauss",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/bwein-net/contao-migrate-short-urls/issues",
    "source": "https://github.com/bwein-net/contao-migrate-short-urls"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/rabauss"
    }
  ],
  "require": {
    "php": "^7.2 || ^8.0",
    "contao/core-bundle": "^4.9",
    "symfony/config": "^4.4 || ^5.2",
    "symfony/dependency-injection": "^4.4 || ^5.2",
    "symfony/http-kernel": "^4.4 || ^5.2",
    "terminal42/contao-url-rewrite": "^1.5.0"
  },
  "require-dev": {
    "contao/easy-coding-standard": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Bwein\\MigrateShortUrls\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "Bwein\\MigrateShortUrls\\ContaoManager\\Plugin"
  },
  "scripts": {
    "ecs": [
      "vendor/bin/ecs check src/ --fix --ansi"
    ]
  }
}
